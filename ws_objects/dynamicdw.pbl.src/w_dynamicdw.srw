								"Specify Filter",&
								"Specify Sort Columns",&
								"DataWindow",&
								"Appeon Web Library",&
								"DataWindow Error",&
								"Alert",&
								"Specify Retrieval Arguments",&
								"Specify Retrieval Criteria",&
								"Microsoft Internet Explorer"}

uint  lu_val,lu_val1

int i
for i = 1 to upperbound(ls_string)
	lu_val = FindWindowA(0, ls_string[i])


	if lu_val>0 then
		if i = 2 or i = 3 then 
			lu_val1 = FindWindowExA(lu_val,0, 0,"OK") 
			send(lu_val1,256,32,0)
			send(lu_val1,257,32,0)
		elseif i = 10 then
			lu_val1 = FindWindowExA(lu_val,0, 0,"&No") 
			if lu_val1 > 0 then
				send(lu_val1,256,32,0)
				send(lu_val1,257,32,0)
