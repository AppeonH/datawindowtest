public subroutine wf_syntaxtest (integer as_testitems);int		i
string 	ls_dataobject,ls_describe,ls_syntax
string 	ls_PbErrorBufer
boolean 	lb_step
long ll_item
ll_item = long(em_1.text)
if ll_item <> 0 then lb_step = true

choose case as_testitems
	case 2
		for i = 1 to 19
			if lb_step then i = ll_item
			ls_describe = string(i,"00 ") 
			ls_syntax = wf_syntax_exporthead(i,ls_PbErrorBufer)
			wf_createdw(ls_describe,ls_dataobject,ls_syntax,lb_step,ls_PbErrorBufer)
			if lb_step then exit
		next
	case 3	
			
		for i = 1 to 21
			if lb_step then i = ll_item
			ls_describe = string(i,"00 ") 
			ls_syntax = wf_syntax_footer(i,ls_PbErrorBufer)
			wf_createdw(ls_describe,ls_dataobject,ls_syntax,lb_step,ls_PbErrorBufer)
			if lb_step then exit
		next
	case 4
		for i = 1 to 18
			if lb_step = true then i = ll_item
			ls_describe = string(i,"00 ") 
			ls_syntax = wf_syntax_table(i,ls_PbErrorBufer)
			wf_createdw(ls_describe,ls_dataobject,ls_syntax,lb_step,ls_PbErrorBufer)
			if lb_step = true then exit
		next
	case 5
		for i = 1 to 23
			if lb_step = true then i = ll_item
			ls_describe = string(i,"00 ") 
			ls_syntax = wf_syntax_group(i,ls_PbErrorBufer)
			wf_createdw(ls_describe,ls_dataobject,ls_syntax,lb_step,ls_PbErrorBufer)
			if lb_step = true then exit
		next
	case 6
		for i = 1 to 8
			if lb_step = true then i = ll_item
			ls_describe = string(i,"00 ") 
			ls_syntax = wf_syntax_dwo(i,ls_PbErrorBufer)
			wf_createdw(ls_describe,ls_dataobject,ls_syntax,lb_step,ls_PbErrorBufer)
			if lb_step = true then exit
		next
	
	
end choose


end subroutine

