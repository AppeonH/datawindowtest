public function string wf_get_dwo_attributes (string as_dwo);
CHOOSE CASE lower(as_dwo)
	CASE 'button'
		RETURN dw_event.describe( 'b_1.attributes' )
		
	CASE 'text'
		RETURN dw_event.describe( 't_1.attributes' )
		
	CASE 'bitmap'
		RETURN dw_event.describe( 'p_1.attributes' )
		
	CASE 'groupbox'
		RETURN dw_event.describe( 'gb_1.attributes' )
		
	CASE 'line'
		RETURN dw_event.describe( 'l_1.attributes' )
		
	CASE 'ellipse'
		RETURN dw_event.describe( 'oval_1.attributes' )
		
	CASE 'rectangle'
		RETURN dw_event.describe( 'r_1.attributes' )
		
	CASE 'roundrectangle'
		RETURN dw_event.describe( 'rr_1.attributes' )
		
	CASE 'column'
		RETURN dw_event.describe( 'id.attributes' )
		
	CASE 'compute'
		RETURN dw_event.describe( 'compute_1.attributes' )
		
	CASE 'report'
		RETURN dw_event.describe( 'dw_event.attributes' )
		
	CASE ELSE
		RETURN ''

END CHOOSE

end function

public subroutine wf_createtext (integer ai_attribute);long i,j,k,m,n
string ls_create[]
string ls_objects[]={		"Text",					"Button",				"Bitmap",&				
									"Compute",				"Line",					"Column"}

String ls_prop[],ls_props,ls_init[]
String ls_value[],ls_type[]
String ls_handle
dw_1.dataobject = "d_freeform"
dw_1.settransobject(sqlca)
dw_1.retrieve()
dw_event.dataobject = "d_attributes"
wf_setkey()
for i = 2 to UpperBound(ls_objects)

	ls_prop[] = ls_init
	ls_value[] = ls_init
	ls_type[] = ls_init
	if ai_attribute = 1 then
		//±ØÒªÊôÐÔ
		CHOOSE CASE lower(ls_objects[i])
			CASE 'button'
				ls_props = "X,Y,Width,Height,Band"
			CASE 'text'
				ls_props = "X,Y,Width,Height,Text,Band"
			CASE 'bitmap'
				ls_props = "X,Y,Width,Height,Band"
			CASE 'groupbox'
				ls_props = "X,Y,Width,Height,Band"
			CASE 'line'
				ls_props = 'band,x1,x2,y1,y2'
			CASE 'ellipse'
				ls_props = "X,Y,Width,Height,Band"
			CASE 'rectangle'
				ls_props = "X,Y,Width,Height,Band"
			CASE 'roundrectangle'
				ls_props = "X,Y,Width,Height,Band"
			CASE 'column'
				ls_props = "X,Y,Width,Height,ID,Band"
			CASE 'compute'
				ls_props = "Expression,X,Y,Width,Height,Band"
			CASE 'report'
				ls_props = "X,Y,Width,Height,Band"
			CASE ELSE
		END CHOOSE
		f_str_to_array ( Trim(ls_props) ,',',ls_prop[])
		
	elseif ai_attribute = 2 then
	
		//Ò»°ãÊôÐÔ
		CHOOSE CASE lower(ls_objects[i])
			CASE 'button'
				ls_props = 'name,band,x,y,width,height,color,text,background.color'
			CASE 'text'
				ls_props = 'name,band,x,y,width,height,color,text,background.color'
			CASE 'bitmap'
				ls_props = 'name,band,x,y,width,height,filename'
			CASE 'groupbox'
				ls_props = 'name,band,x,y,width,height,color,background.color'
			CASE 'line'
				ls_props = 'name,band,x1,x2,y1,y2'
			CASE 'ellipse'
				ls_props = 'name,band,x,y,width,height,background.color'
			CASE 'rectangle'
				ls_props = 'name,band,x,y,width,height,background.color'
			CASE 'roundrectangle'
				ls_props = 'name,band,x,y,width,height,background.color,ellipseheight,ellipsewidth'
			CASE 'column'
				ls_props = 'name,band,id,x,y,width,height,EDIT.CODETABLE,EDIT.FORMAT,background.color'
			CASE 'compute'
				ls_props = 'name,band,x,y,width,height,color,EXPRESSION,background.color'
			CASE 'report'
				ls_props = 'name,band,x,y,width,height,DATAOBJECT'
			CASE ELSE
		END CHOOSE
		f_str_to_array ( Trim(ls_props) ,',',ls_prop[])
		
	elseif ai_attribute = 3 then
		//ËùÓÐÊôÐÔ
		ls_props = wf_get_dwo_attributes(ls_objects[i])
		IF ls_props <> '' THEN
			f_str_to_array ( Trim(ls_props) ,'~t',ls_prop[])
		END IF
	end if
	

	for n = 1 to UpperBound(ls_prop)
		ls_create[n] = 'Create '+ls_objects[i]+'('
	next
	FOR j = 1 to UpperBound(ls_prop)
	
		//ÎÞÐ§ÊôÐÔ
		choose case ls_prop[j]
			case 'attributes','type','coltype','dbname','compute','edit.style','identity', &
				'initial','key','html.linkargs','useinternalleading','update','validation',&
				'validationmsg','values','updatewhereclause','criteria.dialog','criteria.override_edit','criteria.required'
				CONTINUE
		end choose
				
			
		
		//È¡ÊôÐÔÖµ
		ls_handle = f_get_handle( lower(is_keyvalue), lower(ls_prop[j]),';')
		IF ls_handle <> '' THEN
			f_str_to_array( ls_handle,',',ls_value[])
		END IF
		
		//È¡ÀàÐÍ
		ls_handle = f_get_handle( lower(is_keytype), lower(ls_prop[j]),';')
		IF ls_handle<>'' THEN 
			f_str_to_array(ls_handle,',',ls_type[])
		END IF
		
		for k = 1 to UpperBound(ls_prop)
			//Ã¿¸öÊôÐÔÓÐËÄ¸öÖµ£¬²»×ãËÄ¸öÓÃ×î´óµÄ×éºÏ
			m = k
			if k >= upperbound(ls_value) then m = 1//upperbound(ls_value)
			
			if ls_type[2] = "1" then
				ls_create[k] += ls_prop[j] + " = '"+ls_value[m]+"' "
			else
				ls_create[k] += ls_prop[j] + " = "+ls_value[m]+" "
			end if
			//if k <> 4 then ls_create[k] += ","
		NEXT
	NEXT

	for n = 1 to UpperBound(ls_prop)
		ls_create[n] += ')'
	next
	
	string ls_ret
	string ls_error
	for n = 1 to UpperBound(ls_prop)
		ls_ret = dw_1.modify(ls_create[n])
		mle_syntax.text += ls_create[n] + "~r~n" +ls_ret+ "~r~n" + "~r~n"
	next
	
next 








