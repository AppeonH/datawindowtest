public subroutine wf_alltypedwtest ();int		i
string 	ls_dataobject,ls_describe,ls_syntax
boolean 	lb_step
string	ls_errorbuffer
i = long(em_1.text)
if i <> 0 then
	lb_step = true
	ls_dataobject= wf_allstyledw(i,ls_describe,ls_syntax,ls_errorbuffer)
	wf_createdw(ls_describe,ls_dataobject,ls_syntax,lb_step,ls_errorbuffer)
else
	for i = 1 to 16
		ls_describe = ""
		ls_dataobject= wf_allstyledw(i,ls_describe,ls_syntax,ls_errorbuffer)
		wf_createdw(ls_describe,ls_dataobject,ls_syntax,lb_step,ls_errorbuffer)
